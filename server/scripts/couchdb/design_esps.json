{
   "_id": "_design/esps",
   "language": "javascript",
   "views": {
       "allESPs": {
           "map": "function(doc) {\n  if (doc.resource && doc.resource === 'Deployment' && doc.esp) {\n    emit(doc.esp,null);\n  }\n}",
           "reduce": "function(keys, values) {\n  return true;\n}"
       },
       "espsInDeployment": {
           "map": "function(doc) {\n  emit(doc.name, doc.esp);\n}"
       },
       "allESPNames": {
           "map": "function(doc) {\n  if (doc.resource && doc.resource === 'Deployment' && doc.esp && doc.esp.name) {\n    emit(doc.esp.name,null);\n  }\n}",
           "reduce": "function(keys, values) {\n  return true;\n}"
       },
       "espNamesInDeployment": {
           "map": "function(doc) {\n  emit(doc.name, doc.esp.name);\n}"
       }
   }
}